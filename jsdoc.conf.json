{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 20,
  "opts": {
    "recurse": true
  },
  "source": {
    "exclude": ["./dist/"],
    "include": ["./src/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  }
}
